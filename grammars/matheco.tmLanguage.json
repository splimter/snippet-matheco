{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "matheco",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.matheco",
				"match": "\\b(max|min|sort|add|mul|sub|div|fact|sqrt|pow|exp|log|cos|sin|tan|cosr|sinr|tanr|eq|neq|geq|leq)\\b"
			}]
		}
	},
	"scopeName": "source.meo"
}